  <bpmn:process id="Process_BookPublishing" name="Book publishing process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start book publishing">
      <bpmn:outgoing>Flow_1awdhq8</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1awdhq8" sourceRef="StartEvent_1" targetRef="Activity_0jwrc13" />
    <bpmn:userTask id="Activity_0jwrc13" name="Input book data">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="FormField_title" label="Title" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="FormField_genre" label="Genre" type="enum">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="FormField_synopsis" label="Synopsis" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
        </camunda:formData>
        <camunda:taskListener class="rs.ac.uns.ftn.uppservice.service.listeners.AddGenresToEnumField" event="create" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1awdhq8</bpmn:incoming>
      <bpmn:outgoing>Flow_0c81ezi</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0c81ezi" sourceRef="Activity_0jwrc13" targetRef="Activity_0kuz417" />
    <bpmn:serviceTask id="Activity_0kuz417" name="Save data, choose editor and notify chief editor" camunda:class="rs.ac.uns.ftn.uppservice.service.delegates.BookPublishingSaveChooseNotifyDelegate">
      <bpmn:incoming>Flow_0c81ezi</bpmn:incoming>
      <bpmn:outgoing>Flow_0k4bh3y</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0k4bh3y" sourceRef="Activity_0kuz417" targetRef="Activity_0s5b1a1" />
    <bpmn:userTask id="Activity_0s5b1a1" name="Review book title &#38; synopsis" camunda:assignee="${firstReviewAssignee}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="FormField_continueReview" label="Continue review" type="boolean">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="FormField_reason" label="Reason" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0k4bh3y</bpmn:incoming>
      <bpmn:outgoing>Flow_1qfe9ez</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_1q8nkao" name="Continue review?" default="Flow_08vhavv">
      <bpmn:incoming>Flow_1qfe9ez</bpmn:incoming>
      <bpmn:outgoing>Flow_08vhavv</bpmn:outgoing>
      <bpmn:outgoing>Flow_1yc390m</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_1qfe9ez" sourceRef="Activity_0s5b1a1" targetRef="Gateway_1q8nkao" />
    <bpmn:sequenceFlow id="Flow_08vhavv" name="Yes" sourceRef="Gateway_1q8nkao" targetRef="Activity_0796hsr" />
    <bpmn:endEvent id="Event_03jo2bt">
      <bpmn:incoming>Flow_139965t</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0xd9b1r" sourceRef="Activity_0796hsr" targetRef="Activity_1gr9c6j" />
    <bpmn:sequenceFlow id="Flow_1yc390m" name="No" sourceRef="Gateway_1q8nkao" targetRef="Activity_0uyzrtc">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!FormField_continueReview}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Activity_0uyzrtc" name="Reject book and notify writer" camunda:class="rs.ac.uns.ftn.uppservice.service.delegates.RejectBookDelegate">
      <bpmn:incoming>Flow_1yc390m</bpmn:incoming>
      <bpmn:outgoing>Flow_060oysl</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_0796hsr" name="Notify writer to send full book" camunda:class="rs.ac.uns.ftn.uppservice.service.delegates.SendFullBookNotificationDelegate">
      <bpmn:incoming>Flow_08vhavv</bpmn:incoming>
      <bpmn:outgoing>Flow_0xd9b1r</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_1g95jok" name="End process">
      <bpmn:incoming>Flow_060oysl</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_060oysl" sourceRef="Activity_0uyzrtc" targetRef="Event_1g95jok" />
    <bpmn:sequenceFlow id="Flow_139965t" sourceRef="Activity_1gr9c6j" targetRef="Event_03jo2bt" />
    <bpmn:userTask id="Activity_1gr9c6j" name="Send full book">
      <bpmn:incoming>Flow_0xd9b1r</bpmn:incoming>
      <bpmn:outgoing>Flow_139965t</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:boundaryEvent id="Event_1qis248" attachedToRef="Activity_1gr9c6j">
      <bpmn:outgoing>Flow_0vdp993</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerEventDefinition_12mb190">
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT20S</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    <bpmn:sequenceFlow id="Flow_0vdp993" name="Writer didn&#39;t send full book" sourceRef="Event_1qis248" targetRef="Activity_05pdmzm" />
    <bpmn:serviceTask id="Activity_05pdmzm" name="Notify writer and remove data" camunda:class="rs.ac.uns.ftn.uppservice.service.delegates.SendFullBookTimedOutDelegate">
      <bpmn:incoming>Flow_0vdp993</bpmn:incoming>
      <bpmn:outgoing>Flow_1cvsqb9</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_0todkjz" name="End process">
      <bpmn:incoming>Flow_1cvsqb9</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1cvsqb9" sourceRef="Activity_05pdmzm" targetRef="Event_0todkjz" />
    <bpmn:textAnnotation id="TextAnnotation_15a0bpk">
      <bpmn:text>Endpoint that is used for submit will set process variable "initBookFormData" that represents form data</bpmn:text>
