    <bpmn:textAnnotation id="TextAnnotation_0xyex1h">
      <bpmn:text>1. Korisnik ulaze zalbu na plagijarizam pri cemu unosi naslov knjige koja je plagijat kao i ime njenog pisca</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_1ru1j01">
      <bpmn:text>2. Kreiranje Complaint objekta i obavestavanje glavnog urednika da je kreirana zalba</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_09rnrav">
      <bpmn:text>3. Glavni urednik bira urednike koji ce uporedjivati knjige (bar 2)</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_1nkdygb">
      <bpmn:text>4. Kreira objekat za review (ako treba) i obavestava editore da treba da urade review</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:textAnnotation id="TextAnnotation_0s8bap3">
      <bpmn:text>U spec. ne pise da treba da se uradi notifikacija, ali u poredjenju sa ostatkom sistema mislim da i ovde treba</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_1sgqb10" sourceRef="Activity_0118cqt" targetRef="TextAnnotation_0xyex1h" />
    <bpmn:association id="Association_129mn09" sourceRef="Activity_0ohyata" targetRef="TextAnnotation_1ru1j01" />
    <bpmn:association id="Association_06zk5t4" sourceRef="Activity_03f57k8" targetRef="TextAnnotation_09rnrav" />
