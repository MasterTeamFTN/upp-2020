    <bpmn:textAnnotation id="TextAnnotation_1l8yoq2">
      <bpmn:text>5. Svaki od odabranih urednika moze da skine obe knjige i u obavezi je da napravi beleske o poredjenju u zadatom roku</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_1rzx9jb" sourceRef="Activity_0oin3cz" targetRef="TextAnnotation_1l8yoq2" />
    <bpmn:textAnnotation id="TextAnnotation_0vov0qm">
